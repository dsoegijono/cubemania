package {		import flash.events.Event;		public class SeriesEvent extends Event {				public static const REMOVE:String = "removeSeries"				private var series:Series;				public function SeriesEvent(type:String, series:Series) {			super(type, true, true);			this.series = series;		}				public function getSeries():Series {			return series;		}				override public function clone():Event {			return new SeriesEvent(type, series);		}			}	}